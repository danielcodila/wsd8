DROP TABLE IF EXISTS `locales_source`;
CREATE TABLE `locales_source` (   `lid` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Unique identifier of this string.',   `source` blob NOT NULL COMMENT 'The original string in English.',   `context` varchar(255) CHARACTER SET ascii NOT NULL DEFAULT '' COMMENT 'The context this string applies to.',   `version` varchar(20) CHARACTER SET ascii NOT NULL DEFAULT 'none' COMMENT 'Version of Drupal where the string was last used (for locales optimization).',   PRIMARY KEY (`lid`),   KEY `source_context` (`source`(30),`context`) ) ENGINE=InnoDB AUTO_INCREMENT=7340 DEFAULT CHARSET=utf8mb4 COMMENT='List of English source strings.' AUTO_INCREMENT=7340;
