DROP TABLE IF EXISTS `menu_link_content`;
CREATE TABLE `menu_link_content` (   `id` int(10) unsigned NOT NULL AUTO_INCREMENT,   `bundle` varchar(32) CHARACTER SET ascii NOT NULL,   `uuid` varchar(128) CHARACTER SET ascii NOT NULL,   `langcode` varchar(12) CHARACTER SET ascii NOT NULL,   PRIMARY KEY (`id`),   UNIQUE KEY `menu_link_content_field__uuid__value` (`uuid`) ) ENGINE=InnoDB AUTO_INCREMENT=18 DEFAULT CHARSET=utf8mb4 COMMENT='The base table for menu_link_content entities.' AUTO_INCREMENT=18;
