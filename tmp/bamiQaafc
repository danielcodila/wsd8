DROP TABLE IF EXISTS `node`;
CREATE TABLE `node` (   `nid` int(10) unsigned NOT NULL AUTO_INCREMENT,   `vid` int(10) unsigned DEFAULT NULL,   `type` varchar(32) CHARACTER SET ascii NOT NULL COMMENT 'The ID of the target entity.',   `uuid` varchar(128) CHARACTER SET ascii NOT NULL,   `langcode` varchar(12) CHARACTER SET ascii NOT NULL,   PRIMARY KEY (`nid`),   UNIQUE KEY `node_field__uuid__value` (`uuid`),   UNIQUE KEY `node__vid` (`vid`),   KEY `node_field__type__target_id` (`type`) ) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=utf8mb4 COMMENT='The base table for node entities.' AUTO_INCREMENT=19;
